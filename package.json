{
  "name": "fcc-isqa",
  "version": "1.0.0",
  "description": "Implementation of the quality assurance projects for freecodecamp",
  "main": "./src/server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "testconverter": "cross-env NODE_ENV=test mocha  --require @babel/register  \"./src/server/__tests__/metricconvert.test\" --exit --timeout 30000",
    "testissueprojects": "cross-env NODE_ENV=test mocha  --require @babel/register \"./src/server/__tests__/issuesproject.test\" --exit --timeout 30000",
    "testissues": "cross-env NODE_ENV=test mocha  --require @babel/register \"./src/server/__tests__/issues.test.js\" --exit --timeout 30000",
    "testlibrary": "cross-env NODE_ENV=test mocha  --require @babel/register \"./src/server/__tests__/library.test.js\" --exit --timeout 30000",
    "teststock": "cross-env NODE_ENV=test mocha  --require @babel/register \"./src/server/__tests__/stocksearch.test.js\" --exit --timeout 30000",
    "testmessageboard": "cross-env NODE_ENV=test mocha  --require @babel/register \"./src/server/__tests__/messageboards.test.js\" --exit --timeout 30000",
    "testmessageboardthreads": "cross-env NODE_ENV=test mocha  --require @babel/register  \"./src/server/__tests__/messageboardsthreads.test.js\" --exit --timeout 30000",
    "testmessageboardreplies": "cross-env NODE_ENV=test mocha  --require @babel/register  \"./src/server/__tests__/messageboardsthreadsreplies.test.js\" --exit --timeout 30000",
    "testserver": "cross-env NODE_ENV=test mocha  --require @babel/register \"./src/server/__tests__/**/*.js\" --exit --timeout 30000",
    "devserver": "nodemon --exec babel-node ./src/server/server.js",
    "devclient": "parcel ./src/client/index.html",
    "devmode": "concurrently \"nodemon ./src/server/server.js --exec babel-node\" \"parcel ./src/client/index.html\" ",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet -o linterrors.txt --ignore-path .gitignore",
    "repocommit": "npx git-cz",
    "buildserver": "npx babel ./src/server/server.js -o ./lib/server.bundle.js --presets minify",
    "buildlogger": "npx babel ./src/server/logger.js -o ./lib/logger.js --presets minify",
    "buildmodels": "npx babel ./src/server/models/ --out-dir ./lib --presets minify",
    "buildcontrollers": "npx babel ./src/server/controllers/ --out-dir ./lib --presets minify",
    "buildserverdeps": "concurrently \"npm:buildserver\" \"npm:buildlogger\" \"npm:buildmodels\" \"npm:buildcontrollers\"",
    "buildclient": "parcel build ./src/client/index.html --public-url ./",
    "postinstall": "npm run buildserverdeps && npm run buildclient",
    "start": "node ./lib/server.bundle.js",
    "clean": "rimraf .cache dist/*",
    "storybook": "cross-env NODE_ENV=stories start-storybook -p 6006",
    "storybook-build": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonniebigodes/fcc-isqa.git"
  },
  "keywords": [
    "react",
    "node",
    "quality",
    "assurance",
    "heroku"
  ],
  "author": "jonniebigodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonniebigodes/fcc-isqa/issues"
  },
  "engines": {
    "node": "10.13.0",
    "npm":"6.4.1"
  },
  "homepage": "https://github.com/jonniebigodes/fcc-isqa#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.21.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "helmet": "^3.15.1",
    "memory-cache": "^0.2.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.18",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.12.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-knobs": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/addons": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "babel-jest": "^24.3.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "commitizen": "^3.0.7",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^6.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.3.1",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.4",
    "rimraf": "^2.6.3",
    "storybook-react-router": "^1.0.2",
    "superagent": "^4.1.0"
  },
  "targets":{
    "browsers": ["last 2 versions", "safari >= 7"],
	  "node": "6.10"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/enzymeconfig.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
