{
  "name": "fcc-isqa",
  "version": "1.0.0",
  "description": "Implementation of the quality assurance projects for freecodecamp",
  "main": "./src/server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "testissueprojects": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill \"./src/server/__tests__/issuesproject.test\" --exit --timeout 30000",
    "testissues": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill \"./src/server/__tests__/issues.test.js\" --exit --timeout 30000",
    "testlibrary": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill \"./src/server/__tests__/library.test.js\" --exit --timeout 30000",
    "testmessageboard": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill \"./src/server/__tests__/messageboards.test.js\" --exit --timeout 30000",
    "testmessageboardthreads": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill \"./src/server/__tests__/messageboardsthreads.test.js\" --exit --timeout 30000",
    "testmessageboardreplies": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill \"./src/server/__tests__/messageboardsthreadsreplies.test.js\" --exit --timeout 30000",
    "testserver": "mocha --compilers js:babel-core/register --require babel-polyfill \"./src/server/__tests__/**/*.js\" --exit --timeout 30000",
    "devserver": "nodemon ./src/server/server.js --exec babel-node",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "commit": "git-cz",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonniebigodes/fcc-isqa.git"
  },
  "keywords": [
    "react",
    "node",
    "quality",
    "assurance",
    "heroku"
  ],
  "author": "jonniebigodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonniebigodes/fcc-isqa/issues"
  },
  "engines": {
    "node": "8.11.3"
  },
  "homepage": "https://github.com/jonniebigodes/fcc-isqa#readme",
  "dependencies": {
    "@material-ui/core": "^3.0.2",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "helmet": "^3.13.0",
    "memory-cache": "^0.2.0",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.6",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.7",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.10",
    "babel-jest": "^23.4.2",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "commitizen": "^2.10.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.0",
    "react-test-renderer": "^16.4.2",
    "superagent": "^3.8.3"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/enzymeconfig.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
